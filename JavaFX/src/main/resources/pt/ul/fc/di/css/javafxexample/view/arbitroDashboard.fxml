<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<!-- NAO UTILIZADO, MANTER CÓDIGO PARA POSSÍVEIS ALTERAÇÕES FUTURAS-->
<VBox fx:controller="pt.ul.fc.di.css.javafxexample.controller.DashboardController" prefHeight="300.0" prefWidth="230.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.ul.fc.di.css.javafxexample.controller.DashboardController">
  <children>
    <MenuBar prefHeight="5.0" prefWidth="323.0" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem fx:id="menuJogador" mnemonicParsing="false" onAction="#openJogadorMenu" text="Jogador Menu" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="menuEquipa" mnemonicParsing="false" onAction="#openEquipaMenu" text="Equipa Menu" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="menuQuit" mnemonicParsing="false" onAction="#quit" text="Quit" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="334.0" prefWidth="230.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="jogadorMenuButton" layoutX="41.0" layoutY="45.0" mnemonicParsing="false" onAction="#openJogadorMenu" prefHeight="51.0" prefWidth="147.0" text="Jogador Menu" />
            <Button fx:id="equipaMenuButton" layoutX="41.0" layoutY="178.0" mnemonicParsing="false" onAction="#openEquipaMenu" prefHeight="51.0" prefWidth="147.0" text="Equipa Menu" />
            <Button fx:id="quitButton" layoutX="41.0" layoutY="112.0" mnemonicParsing="false" onAction="#quit" prefHeight="51.0" prefWidth="147.0" text="Quit" />
      </children>
    </AnchorPane>
  </children>
</VBox>
